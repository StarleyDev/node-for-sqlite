{
  "name": "ServidorNodeForAngular",
  "version": "2.0.2",
  "author": "Starley Cazorla",
  "description": "Servidor NodeJs para aplicações angular com banco de dados sqlite",
  "private": true,
  "main": "./src/bin/server.js",
  "bin": "./src/bin/server.js",
  "scripts": {
    "start": "node ./src/bin/server.js prod",
    "nodemon": "nodemon ./src/bin/server.js dev node",
    "build": "pkg ."
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http": "0.0.1-security",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.0",
    "mssql": "^9.1.1",
    "node-unzip-2": "^0.2.8",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^4.6.3",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "*/**",
    "outputPath": "dist"
  }
